# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.ExamController.index
POST    /exam                       controllers.ExamController.addExam
GET     /exams                      controllers.ExamController.getExams


GET     /auth                       controllers.Authentication.validateAuthentication(code: String)

GET /fbauth                         controllers.Authentication.authenticate

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
