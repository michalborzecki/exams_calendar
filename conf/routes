GET     /                           controllers.ExamController.index
POST    /addexam                    controllers.ExamController.addExam
GET     /exams                      controllers.ExamController.getExams
GET     /editexam/:id               controllers.ExamController.editExam(id: Long)
POST    /saveexam                   controllers.ExamController.saveExam
GET     /deleteexam/:id             controllers.ExamController.deleteExam(id: Long)

GET     /auth                       controllers.Authentication.validateAuthentication(code: String)

GET /fbauth                         controllers.Authentication.authenticate
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
